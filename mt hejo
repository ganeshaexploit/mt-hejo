-- Load Rayfield
local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()

local Window = Rayfield:CreateWindow({
   Name = "⚡ Auto Summit by Ganesha Exploit",
   LoadingTitle = "Auto Summit",
   LoadingSubtitle = "by Ganesha Exploit",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "AutoSummitConfig"
   },
   Discord = {
      Enabled = false
   },
   KeySystem = false
})

-- Services
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")
local player = Players.LocalPlayer

-- Daftar titik teleport
local teleportPoints = {
    Vector3.new(-532.55, 153.99, 2539.41),
    Vector3.new(661.05, 209.99, 2072.23),
    Vector3.new(-582.07, 153.91, 1887.31),
    Vector3.new(-339.01, 73.36, 2099.93),
    Vector3.new(677.78, 37.04, 1559.28),
    Vector3.new(-249.96, 165.25, 1524.83),
    Vector3.new(177.11, 89.50, 1171.24),
    Vector3.new(587.01, 189.99, 938.16),
    Vector3.new(253.67, 313.99, 959.78),
    Vector3.new(-327.64, 105.22, 1201.46),
    Vector3.new(-2.59, 101.50, 711.37),
    Vector3.new(42.59, 601.50, -240.83),
    Vector3.new(196.16, 601.00, -179.06)
}

-- Variabel global
local teleportEnabled = true
local teleportThread

-- Fungsi notifikasi Roblox
local function Notify(msg)
    StarterGui:SetCore("SendNotification", {
        Title = "⚡ Auto Teleport",
        Text = msg,
        Duration = 3
    })
end

local function SummitNotify()
    StarterGui:SetCore("SendNotification", {
        Title = "⚡ Auto Summit",
        Text = "by Ganesha Exploit",
        Duration = 4
    })
end

-- Fungsi teleport brutal
local function BrutalTeleport()
    while teleportEnabled do
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local hrp = character.HumanoidRootPart

            for _, point in ipairs(teleportPoints) do
                if not teleportEnabled then break end
                hrp.CFrame = CFrame.new(point + Vector3.new(0,3,0))
                task.wait() -- biar tidak freeze
            end

            if teleportEnabled then
                task.wait(1)
                SummitNotify()
                character:BreakJoints()
            end
        end
        player.CharacterAdded:Wait()
        task.wait(1)
    end
end

-- Fungsi toggle
local function ToggleTeleport(state)
    teleportEnabled = state
    Notify("Teleport " .. (teleportEnabled and "Aktif" or "Nonaktif"))

    if teleportEnabled and (not teleportThread or not teleportThread.Running) then
        teleportThread = task.spawn(BrutalTeleport)
    end
end

-- Buat tab dan section
local MainTab = Window:CreateTab("Main")
local TeleportSection = MainTab:CreateSection("Teleport")

-- Tambahkan toggle di UI
MainTab:CreateToggle({
    Name = "Auto Teleport",
    CurrentValue = true,
    Flag = "AutoTeleport",
    Callback = function(Value)
        ToggleTeleport(Value)
    end
})

-- Auto start
Notify("Script Loaded ✅ Auto Start Aktif")
teleportThread = task.spawn(BrutalTeleport)
